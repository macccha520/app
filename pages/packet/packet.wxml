<!--pages/packet.wxml-->
<view class="packet">
  <view class="receive-top">
  	<view class="packet-img">
  		<image src="{{src}}"></image>
  	</view>
  	<view class="packet-user">
  		<view>{{username}}</view>
  		<view>
  			余额: <text class="num">0.00</text>元	
  		</view>
  	</view>
  	<view class="packet-btn" bindtap="extractPage">提现</view>
  </view>
  <view class="packet-nav">
  	<view class="nav-left">
  		<view>我发出的</view>
  		<view class="count">0.00</view>
  		<view>金额(元)</view>
  	</view>
  	<view class="nav-right">
  		<view>我收到的</view>
  		<view class="count">0</view>
  		<view>数量(个)</view>
  	</view>
  </view>
</view>
