/* pages/check.wxss */
.content{
	text-align: center;
}
.check-image{
	width:360rpx;
	height:360rpx;
	margin:160rpx auto;
	margin-bottom: 20px;
}
.check-image image{
	width:100%;
	height:100%;
	border-radius: 100%;
}
.check-text{
	color:#f13612;
	background: rgba(255,255,255,0.6);
	width:600rpx;
	padding:10rpx 0px;
	margin: 100rpx auto;
	margin-bottom: 0px;
}
.check-btn{
	width:600rpx;
	height:86rpx;
	line-height: 86rpx;
	color:#fcd043;
	background: #e2212a;
	margin:35px auto;
	margin-bottom:270rpx;
	border-radius: 5px;
}
