<!--pages/check.wxml-->
<view class="check">
	<view class="logo-container">
      <view class="logo">
        <image src="../../images/timg.jpg"></image>
      </view>
      <view class="user-name">{{userInfo.nickName}}</view>
    </view>
	<view class="check-image">
		<image src="{{imgData.sendBlur}}"></image>
	</view>
	<text class="check-text">{{imgData.sendName}}给你准备新年惊喜</text>
	<view class="check-btn" bindtap="wxPay">塞(随机金额红包)查看</view>
</view>

